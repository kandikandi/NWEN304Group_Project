	<div class="jumbotron">
      <h2>This is the Profile page</h2>
      <div id="items_view">
          <% if results == NULL { %>
            <p><strong>Please Log In!!</strong></p>            
          <% } %>
          <% else { %>
            <% results.forEach(function(data) { %>
              <li><p>Username: <%= data.username %></p></li>
              <li><p>Email: <%= data.email %></p></li>
            <% }); %> 
          <% } %>
              
        
      </div>
    </div>  
</body>

